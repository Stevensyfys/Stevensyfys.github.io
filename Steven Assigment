#Question 1
num = int(input('the quantity of the unit'))
if num >= 10:
    print(num * 100 * 0.9)
else:
    print(num * 100)

#Question 2
a = int(input('enter a 4 digits number'))
b = a%10
c = a//10%10
d = a//10//10%10
e = a//10//10//10%10
print(b,c,d,e)

#Question 3
num = int(input('enter a number'))
if num %7 == 0:
   print('the number can be divided by 7')
else:
   print('the number can not be divided by 7')

#Question 4
mark=int(input("enter your marks"))
if mark < 25:
   print ("D")
elif mark>=25 and mark<45:
   print ("C")
elif mark >=45 and mark<50:
   print ("B")
elif mark >=50 and mark<60:
   print ("B+")
elif mark >=60 and mark <80:
   print ("A")
else:
   print ("A+")

