z=(21, 22, 23, 24, 25)


#Question 6
num =[1,4,62,78,32,23,90,24,2,34]
def count(z):
    num = [1,4,62,78,32,23,90,24,2,34]
    z=len(num)
    print(z)
count(z)

#Question 7
Sample = [2,8,16,60,40,23,32,64,101,76]
sample = [2,8,16,60,40,23,32,64,101,76,44]
list= []
for i in sample:
    if i > 100:
        break
    if i > 50:
        continue
    if i % 4 == 0:
        list.append(i)
print(list)
